
@{
    ViewBag.Title = "Crear";
}
	<!-- sign up-page -->
<div class="login-page">
    <div class="container">
        <h3 class="w3ls-title w3ls-title1">Create your account</h3>
        <div class="login-body">
            <form method="post" action="@Url.Action("RegistrarCliente","Sesion")">


                <input type="text" class="user" name="Nombres" placeholder="Enter your Name" required="">

                <input type="text" class="user" name="Apellidos" placeholder="Enter your Name" required="">

                <input type="text" class="user" name="Usuario" placeholder="Enter your Name" required="">

                <input type="password" name="Clave" class="lock" placeholder="Password" required="">

                <input type="text" class="user" name="Correo" placeholder="Enter your email" required="">

                <input type="text" class="user" name="Celular" placeholder="Enter your Name" required="">

                <input type="text" class="user" name="Direccion" placeholder="Enter your Name" required="">


                <input type="submit" value="Sign Up ">
                <div class="forgot-grid">
                    <label class="checkbox"><input type="checkbox" name="checkbox"><i></i>Remember me</label>
                    <div class="forgot">
                        <a href="#">Forgot Password?</a>
                    </div>
                    <div class="clearfix"> </div>
                </div>
            </form>
        </div>
        <h6>Already have an account? <a href="login.html">Login Now »</a> </h6>
    </div>
</div>
<!-- //sign up-page --> 